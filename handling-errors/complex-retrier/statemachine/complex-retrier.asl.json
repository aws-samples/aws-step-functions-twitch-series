{
  "Comment": "An example of the Amazon States Language with a complex Retry block",
  "StartAt": "Jittery Function",
  "States": {
    "Jittery Function": {
      "Type": "Task",
      "Resource": "${JitteryFunctionArn}",
      "Retry": [
        {
          "ErrorEquals": [ "errorString" ],
          "IntervalSeconds": 5,
          "MaxAttempts": 2,
          "BackoffRate": 2.0
        },
        {
          "ErrorEquals": [ "wrapError" ],
          "IntervalSeconds": 1,
          "MaxAttempts": 10,
          "BackoffRate": 3.0
        }
      ],
      "End": true
    }
  }
}
